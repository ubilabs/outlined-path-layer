{
  "name": "@ubilabs/outlined-path-layer",
  "version": "0.1.0",
  "description": "Custom layer to display paths w/ outlines",
  "author": "Florian Dreyer <dreyer@ubilabs.com>",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "webgl",
    "deckgl",
    "layer"
  ],
  "exports": {
    ".": "./dist/outlined-path-layer.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/outlined-path-layer.mjs",
  "module": "./dist/outlined-path-layer.mjs",
  "types": "./dist/outlined-path-layer.d.mts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && gsutil -m rsync -d -r ./dist_frontend gs://outlinedpathlayer-example && npm run gcs-cache",
    "gcs-cache": "gsutil -m setmeta -r -h \"Cache-Control:no-cache\" gs://outlinedpathlayer-example",
    "build_tsdown": "tsdown",
    "prepublishOnly": "npm run build_tsdown && cp src/outlined-path-layer/README.md README.md",
    "postpublish": "git checkout README.md"
  },
  "devDependencies": {
    "@types/google.maps": "^3.58.1",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vite-plugin-glsl": "^1.5.1",
    "tsdown": "^0.18.1"
  },
  "dependencies": {
    "@deck.gl-community/layers": "^9.1.1",
    "@deck.gl/core": "^9.1.12",
    "@deck.gl/google-maps": "^9.1.14",
    "@deck.gl/layers": "^9.1.12"
  }
}
